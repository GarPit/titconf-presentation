<div id='<%= id %>' class='slide <%= slide_classes.join(' ') %>'
  data-transition='uncover'
  data-sections="<%= hierarchy.reverse.join(',') %>"
  data-sequence="<%= sequence %>">

  <div class='content <%= content_classes.join(' ') %>'>
    <%= content_as_html %>
  </div>
  <div class="branding">
    <img src="/images/Logo.png"/>
  </div>
</div>
